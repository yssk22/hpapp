{
  "name": "@yssk22/hpapp",
  "version": "3.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client --lan",
    "start-go": "yarn expo start",
    "build-dev": "HPAPP_CONFIG_NAME=dev ./scripts/eas-build.sh",
    "build-prod": "HPAPP_CONFIG_NAME=prod yarn eas build --profile prod --platform ios --non-interactive",
    "relay": "node ./scripts/genschema.js && yarn relay-compiler",
    "test": "yarn jest ./",
    "genscreen": "node ./scripts/genscreen.js"
  },
  "relay": {
    "src": "./",
    "schema": "./schema.graphql",
    "language": "typescript",
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ]
  },
  "hpapp": {
    "graphql_version": "v3"
  },
  "dependencies": {
