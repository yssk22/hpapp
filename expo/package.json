{
  "name": "@yssk22/hpapp",
  "version": "3.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client --lan",
    "start-go": "yarn expo start",
    "eas-build-pre-install": "./scripts/eas-build-pre-install.sh",
    "relay": "node ./scripts/genschema.js && yarn relay-compiler",
    "test": "yarn jest ./",
    "lint": "yarn eslint ./",
    "genscreen": "node ./scripts/genscreen.js",
    "start-dev": "HPAPP_CONFIG_NAME=dev expo start --dev-client --lan",
    "build-dev": "HPAPP_CONFIG_NAME=dev ./scripts/eas.sh build --profile dev --platform ios --non-interactive",
    "build-beta": "HPAPP_CONFIG_NAME=beta ./scripts/eas.sh build --profile beta --non-interactive",
    "build-prod": "HPAPP_CONFIG_NAME=prod ./scripts/eas.sh build --profile prod --non-interactive",
    "update-beta": "HPAPP_CONFIG_NAME=beta ./scripts/eas.sh update --channel beta --non-interactive --auto"
  },
  "relay": {
    "src": "./",
    "schema": "./generated/schema.graphql",
    "language": "typescript",
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ],
    "customScalars": {
      "Time": "string"
    }
  },
  "hpapp": {
    "graphql_version": "v3"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-firebase/app": "^17.4.3",
    "@react-native-firebase/app-check": "^17.4.3",
    "@react-native-firebase/auth": "^17.4.3",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.26",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "expo": "^51.0.14",
    "expo-apple-authentication": "~6.4.1",
    "expo-asset": "~10.0.9",
    "expo-auth-session": "~5.5.2",
    "expo-build-properties": "~0.12.3",
    "expo-calendar": "~13.0.5",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.18",
    "expo-dev-menu": "^5.0.15",
    "expo-device": "~6.0.2",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-secure-store": "~13.0.1",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.17",
    "i18n-js": "^4.3.2",
    "jsdom-jscore-rn": "^0.1.8",
    "react": "18.2.0",
    "react-content-loader": "^7.0.2",
    "react-dom": "18.2.0",
    "react-native": "0.74.2",
    "react-native-pager-view": "6.3.0",
    "react-native-picker": "^4.3.7",
    "react-native-root-toast": "^3.5.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.3.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-web": "~0.19.10",
    "react-native-webview": "13.8.6",
    "react-relay": "^17.0.0",
    "react-test-renderer": "^18.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "@testing-library/react-native": "^12.0.1",
    "@types/jest": "^29.5.1",
    "@types/react": "~18.2.79",
    "@types/react-relay": "^16.0.6",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-relay": "^15.0.0",
    "eas-cli": "5.4.0",
    "eslint": "^8.52.0",
    "eslint-config-universe": "^12.0.0",
    "expo-env-info": "^1.2.0",
    "graphql": "^16.6.0",
    "jest": "^29.5.0",
    "jest-expo": "^51.0.2",
    "prettier": "^3.0.3",
    "relay-compiler": "^15.0.0",
    "typedoc": "^0.25.13",
    "typedoc-plugin-rename-defaults": "^0.7.0",
    "typescript": "~5.3.3"
  },
  "resolutions": {
    "@types/react": "18.2.79"
  }
}
